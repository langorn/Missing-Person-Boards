{
  "extends": "near-sdk-as/asconfig.json",
  "entry": "assembly/index.ts",
  "targets": {
    "debug": {
      "binaryFile": "out/main2.wasm",
      "textFile": "out/main2.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "binaryFile": "out/main.wasm",
